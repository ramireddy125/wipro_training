package com.day_11;
import java.util.TreeMap;
import java.util.Map;
import java.util.Scanner;
class Appointement{
	private String patientName;
	private String id;
	private String disease;
	private String time;
	
	public Appointement(String patientName, String id, String disease, String time) {
		this.patientName = patientName;
		this.id=id;
		this.disease=disease;
		this.time=time;
	}
	public String getId(){
		return id;
	}
	public String getName() {
		return patientName;
	}
	public String getDisease() {
		return disease;
	}
	public String getTime() {
		return time;
	}
	public void setTime(String time) {
		this.time=time;
	}
	public String toString() {
		return "Id:" + id +"Name:"+patientName+"disease:" + disease+"time:"+time;
	}
}
public class TreeMap_Appointment {

	public static void main(String[] args) {
		TreeMap<String, Appointement> patientMap = new TreeMap<>();
		Scanner sc = new Scanner(System.in);
		
		patientMap.put("101",new Appointement("venkat","101","fever","10:00AM"));
		patientMap.put("102",new Appointement("venkat","102","fever","11:00AM"));
		patientMap.put("103",new Appointement("venkat","103","fever","12:00AM"));
		
		System.out.println("\n---All appointiments---");
		for(Map.Entry<String, Appointement> A: patientMap.entrySet()) {
			System.out.println(A);
		}
		String remId= sc.nextLine();
		System.out.println("remove appointment:" + patientMap.remove(remId));
	}

}
