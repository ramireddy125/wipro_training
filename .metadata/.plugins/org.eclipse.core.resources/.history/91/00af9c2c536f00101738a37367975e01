package com.day_12_JDBC;
import java.sql.*;

public class DBConnect {

	public static void main(String[] args) {
		
		try {
			
			Class.forName("com.mysql.cj.jdbc.Driver");
			
			Connection con = DriverManager.getConnection("jdbc:mysql://localhost:3306/studentdb","root","Venkat1122@");


			System.out.println("connected sucess");
			
	
			con.close();
			
		}catch (Exception e) {
			e.printStackTrace();
		}

		
	}

}
