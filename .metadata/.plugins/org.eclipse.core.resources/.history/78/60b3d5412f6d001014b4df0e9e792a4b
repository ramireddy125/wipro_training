package com.day_10;
import java.util.*;

public class HashMap_Ex {

	public static void main(String[] args) {
		HashMap<Integer, String> patientMap = new HashMap<>();
		
		patientMap.put(101, "venkat");
		patientMap.put(102, "venkat");
		patientMap.put(103, "venkat");
		patientMap.put(104, "venkat");
		
		System.out.println(patientMap.get(103));
		
		System.out.println(patientMap.containsKey(103));
		
		System.out.println(patientMap.remove(103));
		
        System.out.println(patientMap.get(103));
		
		System.out.println(patientMap.containsKey(103));
		
		System.out.println(patientMap.keySet());
		
		System.out.println(patientMap.entrySet());
		
		

	}

}
